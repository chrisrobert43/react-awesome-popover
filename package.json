{
	"name": "react-awesome-popover",
	"description": "A smart popover component with animation support for ReactJS",
	"version": "3.1.1",
	"main": "dest/react-awesome-popover.js",
	"repository": "https://github.com/vaheqelyan/react-awesome-popover.git",
